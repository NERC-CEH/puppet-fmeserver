# ************** SAFE FME ENGINE SETTINGS START **************

GLOBAL_SECTION                                                                              \
MACRO_DEF FME_SERVER_ROOT "/opt/safe/fmeserver"                                 \
FME_WORKING_DIR "!FME_SERVER_ROOT!/DefaultResults"                                          \
FME_MAPPING_DIR "!FME_SERVER_ROOT!/Server/repository/"                                      \
FME_RESULT_LIFETIME <%= @results_lifetime %>                                                \
FME_PURGE_INTERVAL <%= @purge_interval %>                                                   \
AUTO_FILE_PREFIX FME_                                                                       \
AUTO_DIR_PREFIX FME_                                                                        \
RECEIVE_TIMEOUT 0                                                                           \
FME_SHARED_RESOURCE_DIR "!FME_SERVER_ROOT!/Resources/shared"                                \
FME_SERVER_LOG_FILE "!FME_SERVER_ROOT!/Logs/engine/current/!FME_INSTANCE_NAME!.log" FALSE        \
SUCCESS_RESPONSE 0:Translation Successful|NumFeaturesOutput=!FME_NUM_FEATURES_OUTPUT!       \
FAILURE_RESPONSE !FME_ERROR_NUMBER!:!FME_ERROR_MSG!                                         \
FME_ENGINE_MEMORY_REDLINE 0.5                                                               \
MACRO_DEF FME_DATA_REPOSITORY "!FME_SERVER_ROOT!/Server/Upload/"                            \
MACRO_DEF FME_SHAREDRESOURCE_LOG "/opt/safe/fmeserver/Logs/"                    \
MACRO_DEF FME_SHAREDRESOURCE_DATA "/opt/safe/fmeserver/Data/"                   \
MACRO_DEF FME_SHAREDRESOURCE_TEMP "/opt/safe/fmeserver/Temp/"                   \
MACRO_DEF FME_SHAREDRESOURCE_BACKUP "/opt/safe/fmeserver/Backup/"

SUB_SECTION FILE_DOWNLOAD_SERVICE                                                           \
FME_TRANSFORMATION_LOG_DIR "!FME_SERVER_ROOT!/Logs/engine/current/jobs"                                    \
FME_TRANSFORMATION_LOG_NAME "!FME_AUTO_FILE_NAME_JOBID!.log"              					\
MACRO_DEF FME_SERVER_DEST_DIR "!FME_AUTO_DIR_NAME!.zip"                                        \
SUCCESS_RESPONSE 0:Translation Successful|NotificationLocation=!FME_AUTO_DIR_NAME!_nw|OutputLocation=!FME_AUTO_DIR_NAME!|ResultRootDir=/fmedatadownload/results|NumFeaturesOutput=!FME_NUM_FEATURES_OUTPUT!|LogFileName=!FME_TRANSFORMATION_LOG_NAME! \
FAILURE_RESPONSE !FME_ERROR_NUMBER!:!FME_ERROR_MSG!|LogFileName=!FME_TRANSFORMATION_LOG_NAME!

